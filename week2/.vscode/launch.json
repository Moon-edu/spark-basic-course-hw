{
    "version": "1.0.0",
    "configurations" : [{
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "envFile": "${workspaceFolder}/.env",
        "env": {
            "PYTHONPATH": "${env:PYTHONPATH}${pathSeparator}${workspaceFolder}/src${pathSeparator}${workspaceFolder}/tests{pathSeparator}${workspaceFolder}"
        }
    }]
  }